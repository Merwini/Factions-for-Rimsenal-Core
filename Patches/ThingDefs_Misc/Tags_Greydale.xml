<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rimsenal - Core</li>
		</mods>
        <match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationConditional">
					<xpath>/Defs/ThingDef[defName="Apparel_SecurityArmor"]/apparel/tags</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_SecurityArmor"]</xpath>
						<value>
							<tags></tags>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_SecurityArmor"]/apparel/tags</xpath>
						<value>
							<li>GD_Defender</li>
							<li>GD_Protector</li>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>/Defs/ThingDef[defName="Apparel_SecurityHelmet"]/apparel/tags</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_SecurityHelmet"]</xpath>
						<value>
							<tags></tags>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_SecurityHelmet"]/apparel/tags</xpath>
						<value>
							<li>GD_Defender</li>
							<li>GD_Protector</li>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>/Defs/ThingDef[defName="Apparel_PioneerArmor"]/apparel/tags</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_PioneerArmor"]</xpath>
						<value>
							<tags></tags>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_PioneerArmor"]/apparel/tags</xpath>
						<value>
							<li>GD_ElitePioneer</li>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>/Defs/ThingDef[defName="Apparel_PioneerH"]/apparel/tags</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_PioneerH"]</xpath>
						<value>
							<tags></tags>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="Apparel_PioneerH"]/apparel/tags</xpath>
						<value>
							<li>GD_ElitePioneer</li>
						</value>
					</match>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="GD_TacticalPistol"]</xpath>
					<value>
						<weaponTags>
							<li>GD_BasicPistol</li>
						</weaponTags>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="GD_MSST"]</xpath>
					<value>
						<weaponTags>
							<li>GD_AutoPistol</li>
						</weaponTags>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="GD_HybridRifle"]</xpath>
					<value>
						<weaponTags>
							<li>GD_HybridRifle</li>
						</weaponTags>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="GD_MSSH"]</xpath>
