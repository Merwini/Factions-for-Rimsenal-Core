						<weaponTags>
							<li>GD_Knife</li>
						</weaponTags>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/thingDef[defName="GD_MSSS"]</xpath>
					<value>
						<weaponTags>
							<li>GD_MachinePistol</li>
						</weaponTags>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/thingDef[defName="GD_HVSMG"]</xpath>
					<value>
						<weaponTags>
							<li>GD_SMG</li>
